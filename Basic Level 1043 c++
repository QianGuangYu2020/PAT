#include <iostream>
using namespace std;
int main(){
    string s;
    cin>>s;
    int n1=0, n2=0, n3=0, n4=0, n5=0 ,n6=0;
    for(int i=0; i<s.length(); i++){
        if(s[i]=='P') n1++;
        else if(s[i]=='A') n2++;
        else if(s[i]=='T') n3++;
        else if(s[i]=='e') n4++;
        else if(s[i]=='s') n5++;
        else if(s[i]=='t') n6++;
    }
    for(int i=0; i<s.length(); i++){
        if(n1>0){
            cout<<'P';
            n1--;
        }
        if(n2>0){
            cout<<'A';
            n2--;
        }
        if(n3>0){
            cout<<'T';
            n3--;
        }
        if(n4>0){
            cout<<'e';
            n4--;
        }
        if(n5>0){
            cout<<'s';
            n5--;
        }
        if(n6>0){
            cout<<'t';
            n6--;
        }
    }
    return 0;
}
