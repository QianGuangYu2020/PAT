#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    string hash[n];
    string s1, s2;
    int idx;
    for(int i=0; i<n; i++){
        cin>>s1>>idx>>s2;
        hash[idx-1]=s1+" "+s2;
    }
    int m;
    cin>>m;
    int temp;
    for(int i=0; i<m; i++){
        cin>>temp;
        cout<<hash[temp-1]<<endl;
    }
    return 0;
}
