#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int x1=0, x2=0, x3=0;
    x1=n/100;
    x2=n%100/10;
    x3=n%10;
    for(int i=0; i<x1; i++){
        cout<<'B';
    }
    for(int i=0; i<x2; i++){
        cout<<'S';
    }
    for(int i=0; i<x3; i++){
        cout<<i+1;
    }
    return 0;
}
